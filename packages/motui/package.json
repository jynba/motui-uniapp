{
  "name": "motui-uniapp",
  "displayName": "motui-uniapp 轻量级移动端 Uniapp、Vue3 组件库",
  "version": "1.0.2",
  "description": "轻量级移动端 Uniapp、Vue3 组件库（支持小程序开发）",
  "author": "motui",
  "license": "MIT",
  "homepage": "https://github.com/jynba/motui-uniapp#readme",
  "repository": "https://github.com/jynba/motui-uniapp.git",
  "bugs": "https://github.com/jynba/motui-uniapp/issues",
  "keywords": [
    "motui",
    "小程序",
    "vue3",
    "暗黑模式",
    "mini-program",
    "vue3 component"
  ],
  "sideEffects": [
    "*.scss",
    "*/components/**/*.vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*",
    "./composables": "./components/composables.ts",
    "./locale": "./locale/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "web-types": "./web-types.json",
  "vetur": {
    "tags": "./tags.json",
    "attributes": "./attributes.json"
  },
  "files": [
    "README.md",
    "attributes.json",
    "components",
    "components/composables.ts",
    "dist",
    "global.d.ts",
    "locale/**",
    "styles",
    "tags.json",
    "web-types.json"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "@uni-helper/uni-app-types": ">=0.5.12",
    "vue": ">=3.2.0"
  }
}
